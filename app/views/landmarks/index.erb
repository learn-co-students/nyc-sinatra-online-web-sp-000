<h2>All Historical Landmarks</h2>

<% @landmarks.each do |landmark| %>
<ul>
  <li><a href="/landmarks/<%= landmark.id %>"> <%= landmark.name %>
    <%= landmark.year_completed %> </a><br/>
  </li>
<% end %>
</ul>
<a class="button" href="/landmarks/new">Create New Landmark</a>
