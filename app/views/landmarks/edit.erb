<h2>Edit Historical Landmark</h2>
  <form action="/landmarks/<%= @landmark.id %>" method="post">
    <input id="hidden" type="hidden" name="_method" value="PATCH"
    <h2><label> Create New Landmark: </label></h2><br>
    Name: <input type="text" name="landmark[name]" id="name" value="<%= @landmark.name %>">
    Year: <input type="text" name="landmark[year_completed]" id="year_completed" value="<%= @landmark.year_completed %>">
    <input type="submit" value="Edit Landmark">
  </form>

<a class="button" href="/landmarks">Landmarks</a>
<a class="button" href="/">Home</a>
