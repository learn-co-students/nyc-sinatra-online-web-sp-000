<table>
  <tr>
    <th>Name</th>
    <th>Year Completed</th>
  </tr>
  <tr>
    <td><%= @landmark.name %></td>
    <td><%= @landmark.year_completed %></td>
  </tr>
<table>
<form action="landmark/<% @landmark.id %>" method="post">
    <input type="text" name="landmark[name]" id="name" value=<%=@landmark.name%> ><br>
    <input type="text" name="landmark[year_completed]" id="year_completed" value=<%=@landmark.year_completed%>><br>
    
    <button type="submit">Edit Landmark</button>
</form>



<a class="button" href="/landmarks/<%= @landmark.id %>/edit">Edit Landmark</a>
<a class="button" href="/landmarks">Landmarks</a>
