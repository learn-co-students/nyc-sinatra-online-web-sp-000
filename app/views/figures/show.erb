<h1><%= @figure.name %></h1>
<h3>Landmarks</h3>
<ol>
<% @figure.landmarks.each do |landmark| %>
  <li><%= landmark.name %></li>
<% end %>
</ol>
<h3>Titles</h3>
<ol>
<% @figure.titles.each do |title| %>
  <li><%= title.name %></li>
<% end %>
</ol>
<a href="/figures/<%= @figure.id %>/edit"><input type="submit" value="Edit this figure"/></a>
