
<h1><%=@figure.name%></h1>
<ul>
    <label>Titles:</label>
    <%@figure.titles.each do |title|%>
    <li><%=title.name%></li>
    <%end%><br>
    <label>Landmarks:</label>
    <%@figure.landmarks.each do |landmark|%>
    <li><%=landmark.name%></li>
    <%end%><br>
</ul>
<a href="/figures/<%=@figure.id%>/edit"><input type="button" value="edit"></a>
